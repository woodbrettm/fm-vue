{
  "name": "Frontend Mentor Vue.js",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "dbaeumer.vscode-eslint",
        "vue.volar",
        "voorjaar.windicss-intellisense",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "forwardPorts": [
    5173,
    4173
  ],
  "portsAttributes": {
    "5173": {
      "label": "Vite Dev Server",
      "onAutoForward": "notify"
    },
    "4173": {
      "label": "Vite Preview Server",
      "onAutoForward": "notify"
    }
  }
